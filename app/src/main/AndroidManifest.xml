<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nokhrin.corners">

    <application
        android:allowBackup="true"
        android:icon="@drawable/for_icon"
        android:label="@string/app_name2"
        android:roundIcon="@drawable/for_icon"
        android:supportsRtl="true"
        android:theme="@style/Theme.Corners">
        <activity
            android:name=".levels.ActivityGameLevel"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_game_level"
            android:theme="@style/Theme.Corners.Fullscreen"></activity>
        <activity
            android:name=".levels.database.ActivityCreateLevel"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_create_level"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".multiplayer.ActivityMultiplayerGame"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_multi3_game"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".multiplayer.ActivityRooms"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_multi2"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".multiplayer.ActivityCreatePlayer"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_multiplayer"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".classical.view.ActivityClassic"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_classic"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".levels.view.ActivityLevels"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_levels"
            android:theme="@style/Theme.Corners.Fullscreen" />
        <activity
            android:name=".ActivityStart"
            android:configChanges="orientation|keyboardHidden|screenSize"
            android:label="@string/title_activity_start"
            android:theme="@style/Theme.Corners.Fullscreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>